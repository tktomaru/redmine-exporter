# PostgreSQL Database Configuration
# データベースのパスワード（必須）
POSTGRES_PASSWORD=redmine_password

# データベース名（必須）
POSTGRES_DB=redmine

# データベースユーザー名（必須）
POSTGRES_USER=redmine

# Redmine Configuration
# Redmineのシークレットキーベース（必須）
# 以下のコマンドでランダムな値を生成できます：
# openssl rand -hex 64
REDMINE_SECRET_KEY_BASE=your_secret_key_base_here_please_generate_random_value

# Redmineのポート番号（オプション、デフォルト: 3000）
REDMINE_PORT=3000
